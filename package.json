{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://murugesharavind.github.io/react-router-prompt_demo/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test:coverage": "bun test --coverage",
    "coverage:html": "lcov-viewer lcov -o ./coverage-html ./coverage/lcov.info",
    "playwright": "npx playwright test demo-test.spec.ts --ui --config=playwright.config.ts",
    "playwright:report": "playwright show-report",
    "predeploy": "bun run build && bun run lint:fix",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@lottiefiles/dotlottie-react": "^0.13.5",
    "@mui/material": "^7.1.0",
    "@tailwindcss/vite": "^4.1.8",
    "@types/bun": "^1.2.15",
    "axios": "^1.9.0",
    "dev": "^0.1.3",
    "gh-pages": "^6.3.0",
    "playwright": "^1.52.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "^7.6.1",
    "react-router-prompt": "^0.8.0",
    "sass": "^1.89.1",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@happy-dom/global-registrator": "^17.5.6",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-fixed-jsdom": "^0.0.9",
    "test-jest": "^1.0.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5"
  }
}
